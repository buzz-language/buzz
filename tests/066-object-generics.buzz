import "std";
import "debug";

object Payload::<K, V> {
    {K, V} data,
}

test "Objects generics" {
    Payload::<str, int> payload = Payload::<str, int>{
        data = {
            "one": 1
        },
    };

    payload.data["two"] = 2;

    assert(payload is Payload::<str, int>, message: "Could use objects generics");
    assert(payload.data["two"] == 2, message: "Could use objects generics");
}