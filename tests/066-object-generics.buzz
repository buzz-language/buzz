import "std";

object Payload::<K, V> {
    data: mut {K: V},
}

test "Objects generics" {
    final payload = Payload::<str, int>{
        data = mut {
            "one": 1
        },
    };

    payload.data["two"] = 2;

    std\assert(payload is Payload::<str, int>, message: "Could use objects generics");
    std\assert(payload.data["two"] == 2, message: "Could use objects generics");
}
