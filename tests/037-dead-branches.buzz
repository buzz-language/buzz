import "std";

test "if" {
    if (true) {
        std.assert(true, message: "only this branch should be generated");
    } else {
        std.assert(false, message: "unreachable");
    }
}

test "foreach" {
    foreach (str _ in {<str: str>}) {
        std.assert(false, message: "unreachable");
    }
}

test "while" {
    while (false) {
        std.assert(false, message: "unreachable");
    }
}

test "for" {
    for (int i = 0; false; i = i + 1) {
        std.assert(false, message: "unreachable");
    }
}