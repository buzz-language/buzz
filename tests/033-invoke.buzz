import "std";

object A {
    [A] list,

    fun hello() > A {
        print("hello");
        return this;
    }
}

test "Chained invoke" {
    A a = A{
        list = [
            A{
                list = [ A{ list = [<A>] } ]
            },
            A{
                list = [<A>]
            },
        ]
    };

    a.list[0].hello().list[0].hello();
}